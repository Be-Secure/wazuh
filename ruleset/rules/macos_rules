<rule id="100020" level="5">
 <decoded_as>macOS_tcc_process</decoded_as>
 <match type="pcre2">(?i)update access record.+allowed</match>
 <description>$(application) has been granted permission to $(service) at $(time_access_granted)</description>
</rule>

<rule id="100021" level="5">
 <decoded_as>macOS_tcc_process</decoded_as>
 <match type="pcre2">(?i)update access record.+denied</match>
 <description>$(application) has been denied permission to $(service) at $(time_access_denied)</description>
 </rule>
</group>
